{
    "name": "piedweb/qwanturank",
    "type": "library",
    "description": "Harvest statistics and meta data from an URL or his source code (seo oriented).",
    "keywords": [
        "Pied Web",
        "qwanturank"
    ],
    "homepage": "https://qwanturankseo.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Qwanturank",
            "email": "contact@qwanturankseo.com",
            "homepage": "https://qwanturankseo.com"
        }
    ],
    "require": {
        "php" : "~7.2",
        "ropendev/google": "^0.0.3"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "PiedWeb\\Qwanturank\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PiedWeb\\Qwanturank\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    }
}
